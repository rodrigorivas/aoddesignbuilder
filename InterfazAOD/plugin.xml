<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="false"
            id="InterfazAOD.actionSet">
          
        <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/Setting.png"
               class="interfazaod.actions.ClasesAction"
               tooltip="AOD Builder"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="interfazaod.actions.ClasesAction">
         </action>
         
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="AOD Perspective"
            icon="icons/releng_gears.gif"
            class="interfazaod.perspectives.AODPerspective"
            id="interfazaod.perspectives.AODPerspective">
      </perspective>
   </extension>

</plugin>
